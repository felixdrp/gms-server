    "express-graphql": "^0.4.9",
    "graphql": "^0.4.18",
    "history": "^1.17.0",
    "multer": "^1.1.0",
    "qs": "^6.0.2",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.6"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-jest": "^9.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-export-extensions": "^6.5.0",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
